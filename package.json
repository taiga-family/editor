{
    "name": "editor",
    "version": "4.11.0",
    "private": true,
    "description": "Rich text editor extension with related Taiga UI components",
    "keywords": [
        "wysiwyg",
        "text editor",
        "taiga ui",
        "prosemirror",
        "tiptap"
    ],
    "homepage": "https://taiga-family.github.io/editor",
    "bugs": "https://github.com/tahiga-family/editor/issues",
    "repository": "https://github.com/taiga-family/editor",
    "license": "Apache-2.0",
    "contributors": [
        "Alex Inkin <alexander@inkin.ru>"
    ],
    "workspaces": [
        "projects/*"
    ],
    "scripts": {
        "prepare": "husky",
        "start": "nx serve editor-demo",
        "lint": "eslint .",
        "prettier": "prettier !package-lock.json !taiga-ui/** . --ignore-path .gitignore",
        "stylelint": "stylelint './projects/**/*.{less,css}' --config package.json",
        "release": "npx syncer && npx nx publish editor"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.less": [
            "stylelint --fix"
        ],
        "*.{js,ts,html}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{svg,yml,md,less,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all",
            "plugin:@taiga-ui/experience/taiga-naming",
            "plugin:@taiga-ui/experience/entry-points"
        ]
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ]
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "16.0.1",
        "@angular-devkit/build-angular": "16.2.16",
        "@angular-devkit/core": "16.2.16",
        "@angular-devkit/schematics": "16.2.16",
        "@angular/animations": "16.2.12",
        "@angular/cdk": "16.2.14",
        "@angular/cli": "16.2.16",
        "@angular/common": "16.2.12",
        "@angular/compiler": "16.2.12",
        "@angular/compiler-cli": "16.2.12",
        "@angular/core": "16.2.12",
        "@angular/forms": "16.2.12",
        "@angular/language-service": "16.2.12",
        "@angular/platform-browser": "16.2.12",
        "@angular/platform-browser-dynamic": "16.2.12",
        "@angular/platform-server": "16.2.12",
        "@angular/router": "16.2.12",
        "@ng-web-apis/common": "4.6.4",
        "@ng-web-apis/universal": "4.6.4",
        "@nguniversal/builders": "16.2.0",
        "@nguniversal/express-engine": "16.2.0",
        "@nx/angular": "19.8.2",
        "@nx/jest": "19.8.2",
        "@nx/workspace": "19.8.2",
        "@playwright/test": "1.47.2",
        "@swc-node/register": "1.10.9",
        "@swc/core": "1.7.28",
        "@swc/helpers": "0.5.13",
        "@taiga-ui/addon-doc": "4.8.1",
        "@taiga-ui/auto-changelog-config": "0.219.0",
        "@taiga-ui/browserslist-config": "0.219.0",
        "@taiga-ui/cdk": "4.8.1",
        "@taiga-ui/commitlint-config": "0.219.0",
        "@taiga-ui/core": "4.8.1",
        "@taiga-ui/eslint-plugin-experience": "0.219.0",
        "@taiga-ui/event-plugins": "4.2.3",
        "@taiga-ui/icons": "4.8.1",
        "@taiga-ui/layout": "4.8.1",
        "@taiga-ui/legacy": "4.8.1",
        "@taiga-ui/polymorpheus": "4.7.3",
        "@taiga-ui/prettier-config": "0.219.0",
        "@taiga-ui/release-it-config": "0.219.0",
        "@taiga-ui/stylelint-config": "0.219.0",
        "@taiga-ui/styles": "4.8.1",
        "@taiga-ui/syncer": "0.219.0",
        "@taiga-ui/testing": "4.8.1",
        "@taiga-ui/tsconfig": "0.219.0",
        "@types/express": "5.0.0",
        "@types/jasmine": "5.1.4",
        "@types/jest": "29.5.13",
        "@types/markdown-it": "14.1.2",
        "@types/markdown-it-container": "2.0.10",
        "@types/node": "22.7.3",
        "@types/showdown": "2.0.6",
        "@types/webpack-env": "1.18.5",
        "express": "4.21.0",
        "husky": "9.1.6",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.2.4",
        "lint-staged": "15.2.10",
        "markdown-it": "14.1.0",
        "marked": "14.1.2",
        "ng-morph": "4.8.2",
        "ng-packagr": "16.2.3",
        "ngx-highlightjs": "10.0.0",
        "nx": "19.8.2",
        "showdown": "2.1.0",
        "ts-jest": "29.2.5",
        "tslib": "2.7.0",
        "typescript": "5.0.4",
        "webpack": "5.95.0"
    },
    "syncer": {
        "includePaths": [
            "./projects",
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "@taiga-ui/editor",
            "^editor$"
        ],
        "ignorePackageNames": []
    }
}
